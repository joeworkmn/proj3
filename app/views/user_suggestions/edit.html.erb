<% if cookies.signed[:authenticated] %>
  <% if !(cookies.signed[:admin]) %>
    <%= form_for @usersuggestion, :url => {:controller => "suggestions", :action => "update"} do |f| %>
      <%= f.label "Edit Suggestion: " %><br /><%= f.text_area :suggestion %>
      <%= f.hidden_field :suggestionid %>
      <%= f.submit "Submit" %>
    <% end %>
  <% end %>
<% end %>
